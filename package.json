{
  "name": "zklink-api-web",
  "version": "1.0.0",
  "description": "Top 100 zkLink rich list frontend + Express API (Wayback snapshot support)",
  "main": "server.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch . --ext js,json --exec \"node server.js\"",
    "lint": "eslint . --ext .js",
    "prepare": "npm run lint || true"
  },
  "keywords": [
    "zklink",
    "zkl",
    "rich-list",
    "express",
    "wayback",
    "archive"
  ],
  "license": "MIT",
  "files": [
    "public/",
    "server.js",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "eslint": "^8.54.0"
  }
}
